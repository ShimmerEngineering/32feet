{
  "metadata": [
    {
      "src": [
        {
          "files": [
            	"InTheHand.BluetoothLE/bin/Release/net7.0/InTheHand.BluetoothLE.dll",
 		"InTheHand.Bluetooth.Permissions/bin/Release/net7.0/InTheHand.Bluetooth.Permissions.dll",
		"InTheHand.Net.Bluetooth/bin/Release/net7.0/InTheHand.Net.Bluetooth.dll",
 		"InTheHand.Net.IrDA/bin/Release/**/InTheHand.Net.IrDA.dll",
 		"InTheHand.Net.Obex/bin/Release/**/InTheHand.Net.Obex.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api/"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "_site",
    "filter": "filterConfig.yml",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "sitemap": {
        "baseUrl": "https://inthehand.github.io",
        "changefreq": "monthly"
    },
    "template": [
      "default", "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
        "_appLogoPath": "images/Logo.png",
        "_appName": "32feet.NET",
        "_appTitle": "Personal Area Networking for .NET",
        "_appFooter": "Copyright (c) 2023 In The Hand Ltd."
    }
  }
}